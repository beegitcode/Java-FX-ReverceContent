<?xml version="1.0" encoding="UTF-8"?>
 
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
 
 
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="pl.javastart.fxe.controller.ContentPaneController">
    <children>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <TextArea fx:id="inputTextArea" maxHeight="1.7976931348623157E308"
                    maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0"
                    HBox.hgrow="ALWAYS" />
                <TextArea fx:id="outputTextArea" maxHeight="1.7976931348623157E308"
                    maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0"
                    HBox.hgrow="ALWAYS" />
            </children>
        </HBox>
        <Button fx:id="clearButton" maxWidth="1.7976931348623157E308"
            mnemonicParsing="false" text="Clear windows" VBox.vgrow="NEVER" />
    </children>
</VBox>